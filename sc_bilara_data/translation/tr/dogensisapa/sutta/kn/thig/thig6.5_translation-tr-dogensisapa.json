{
  "thig6.5:0.2": "Altılar Silsilesi ",
  "thig6.5:0.3": "Anopamā'nın Şarkısı ",
  "thig6.5:1.1": "Para pul, mal mülk sahibi ",
  "thig6.5:1.2": "Seçkin bir aileye doğdum. ",
  "thig6.5:1.3": "Tenimin rengi ve hoş görünümümle ",
  "thig6.5:1.4": "Majjha'nın öz kızıydım. ",
  "thig6.5:2.1": "Kralların oğulları peşimdeydi, ",
  "thig6.5:2.2": "Varlıklı oğullar beni arzulardı. ",
  "thig6.5:2.3": "Biri de babama elçi yolladı: ",
  "thig6.5:2.4": "“Ver bana Anopamā'yı! ",
  "thig6.5:3.1": "Öz kızın Anopamā'nın ",
  "thig6.5:3.2": "Ne ederse ağırlığı ",
  "thig6.5:3.3": "Sekiz katı vereceğim ",
  "thig6.5:3.4": "Altın ve mücevheratı.” ",
  "thig6.5:4.1": "Ama dünyanın önde geleni ",
  "thig6.5:4.2": "O eşsiz Aydın'ı görünce ",
  "thig6.5:4.3": "Saygıyla ayaklarına kapandım, ",
  "thig6.5:4.4": "Yanına çöküp oturdum. ",
  "thig6.5:5.1": "Hakikati öğretti Dosdoğru Gören ",
  "thig6.5:5.2": "Merhamet eyleyip bana. ",
  "thig6.5:5.3": "Orada otururken hem ",
  "thig6.5:5.4": "Üçüncü meyveye dokundum. ",
  "thig6.5:6.1": "Böylece saçımı kesip ",
  "thig6.5:6.2": "Evsiz yola düştüm. ",
  "thig6.5:6.3": "Yedinci gece oluyor ",
  "thig6.5:6.4": "Artık tutku kaybolalı. ",
  "thig6.5:7.1": "… Kıdemli dervişe Anopamā …. "
}