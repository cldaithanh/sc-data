{
  "thig6.3:0.1": "Kıdemli Dervişelerin Şarkıları 6.3 ",
  "thig6.3:0.2": "Altılar Silsilesi ",
  "thig6.3:0.3": "Khemā'nın Şarkısı ",
  "thig6.3:1.1": "“Sen de gençsin ve güzelsin, ",
  "thig6.3:1.2": "Ben de genç bir delikanlıyım. ",
  "thig6.3:1.3": "Gel şu beşli çalgıcıları ",
  "thig6.3:1.4": "Dinleyip mest olalım.” ",
  "thig6.3:2.1": "“Bu çürüşmüş beden ",
  "thig6.3:2.2": "Hasta ve kırılgan; ",
  "thig6.3:2.3": "İğrenip tiksindim; ",
  "thig6.3:2.4": "Şevkleri yok ettim. ",
  "thig6.3:3.1": "Kılıç gibi mızrak gibi arzular, ",
  "thig6.3:3.2": "Kesme tahtası da kümeler; ",
  "thig6.3:3.3": "'Haz' dediğin cinsellikten ",
  "thig6.3:3.4": "Hazzetmem ki hiç ben. ",
  "thig6.3:4.1": "Yokoldu bütün kabuller; ",
  "thig6.3:4.2": "Parçalandı kara kümeler; ",
  "thig6.3:4.3": "Bunu da bil işte hain: ",
  "thig6.3:4.4": "Ey kâtil, sen yenildin! ",
  "thig6.3:5.1": "Yıldızlara hürmet eder, ",
  "thig6.3:5.2": "Korularda ateş yakar, ",
  "thig6.3:5.3": "Neyin ne olduğunu bilmez ",
  "thig6.3:5.4": "Aptalca kutsal sanırdım. ",
  "thig6.3:6.1": "Şimdi de hürmet ederim ",
  "thig6.3:6.2": "En güzel insan Aydın'a; ",
  "thig6.3:6.3": "Tümden çileden arındım ",
  "thig6.3:6.4": "Vardım Üstad'ın yoluna.” ",
  "thig6.3:7.1": "… Kıdemli dervişe Khemā …. "
}