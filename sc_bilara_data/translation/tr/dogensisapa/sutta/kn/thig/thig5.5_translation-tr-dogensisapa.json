{
  "thig5.5:0.1": "Kıdemli Dervişelerin Şarkıları 5.5 ",
  "thig5.5:0.2": "Beşler Silsilesi ",
  "thig5.5:0.3": "Nanduttarā'nın Şarkısı ",
  "thig5.5:1.1": "Ateşe, güneşe, aya- ",
  "thig5.5:1.2": "Tapıyordum tanrılara. ",
  "thig5.5:1.3": "Gidip nehir kenarına ",
  "thig5.5:1.4": "Teslim oluyordum suya. ",
  "thig5.5:2.1": "Nice adaklar adadım, ",
  "thig5.5:2.2": "Yarı saçımı kazıdım, ",
  "thig5.5:2.3": "Yerde yatak hazırladım, ",
  "thig5.5:2.4": "Gece yemekten kaçındım. ",
  "thig5.5:3.1": "Süslenip takılarımla, ",
  "thig5.5:3.2": "Banyolar ve masajlarla ",
  "thig5.5:3.3": "Şımarttım bedenimi ",
  "thig5.5:3.4": "Şevklerin kucağında. ",
  "thig5.5:4.1": "Nihayet imana gelip, ",
  "thig5.5:4.2": "Evsiz yola düşüverdim. ",
  "thig5.5:4.3": "Bedeni olduğu gibi ",
  "thig5.5:4.4": "Gördüm, tükendi şevkim. ",
  "thig5.5:5.1": "Kökten kesildi varoluş, ",
  "thig5.5:5.2": "İstekler ve de niyetler. ",
  "thig5.5:5.3": "Tüm düğümlerden kurtuldum; ",
  "thig5.5:5.4": "Sükûnet buldu gönlüm. ",
  "thig5.5:6.1": "… Kıdemli dervişe Nanduttarā …. "
}