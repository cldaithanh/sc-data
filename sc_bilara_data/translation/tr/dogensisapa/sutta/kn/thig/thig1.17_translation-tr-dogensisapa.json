{
  "thig1.17:0.1": "Kıdemli Dervişelerin Şarkıları 1.17 ",
  "thig1.17:0.2": "Birler Silsilesi ",
  "thig1.17:0.3": "Dhammā'nın Şarkısı ",
  "thig1.17:1.1": "Sadaka için gezindim ",
  "thig1.17:1.2": "Zar-zor âsâma dayanıp. ",
  "thig1.17:1.3": "Titredi ayağım elim, ",
  "thig1.17:1.4": "Oracıkta devriliverdim. ",
  "thig1.17:1.5": "Bedeni engel diye görünce, ",
  "thig1.17:1.6": "Bir anda kurtuldu gönlüm. ",
  "thig1.17:2.1": "… Kıdemli dervişe Dhammā …. "
}