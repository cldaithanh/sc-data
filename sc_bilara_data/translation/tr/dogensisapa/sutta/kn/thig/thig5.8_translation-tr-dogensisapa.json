{
  "thig5.8:0.1": "Kıdemli Dervişelerin Şarkıları 5.8 ",
  "thig5.8:0.2": "Beşler Silsilesi ",
  "thig5.8:0.3": "Soṇā'nın Şarkısı ",
  "thig5.8:1.1": "On oğlan doğurdum ",
  "thig5.8:1.2": "Beden denen bu biçimde. ",
  "thig5.8:1.3": "Sonra güçsüz bir ihtiyarken ",
  "thig5.8:1.4": "Yanaştım bir dervişeye. ",
  "thig5.8:2.1": "Bana hakikati anlattı: ",
  "thig5.8:2.2": "Kümeleri, alanları, elementleri. ",
  "thig5.8:2.3": "Saçımı kazıyıp yola düştüm. ",
  "thig5.8:2.4": "Ondan dinleyince hakikati. ",
  "thig5.8:3.1": "İlâhî görüm temizlendi; ",
  "thig5.8:3.2": "Öğrenciliğimde hatırladım ",
  "thig5.8:3.3": "Geçmiş yaşamlarımı, ",
  "thig5.8:3.4": "Nerelerde yaşadığımı. ",
  "thig5.8:4.1": "İmgesizliğe yoğunlaştım ",
  "thig5.8:4.2": "Akl-ı selim bütünlüğüyle; ",
  "thig5.8:4.3": "Anında özgürlüğe kavuşup ",
  "thig5.8:4.4": "Kavramayı bıraktım, söndüm. ",
  "thig5.8:5.1": "Kümeler tamamen anlaşıldı- ",
  "thig5.8:5.2": "Duruyorlar ama kökleri söküldü. ",
  "thig5.8:5.3": "Lanet olasıca pis ihtiyarlık- ",
  "thig5.8:5.4": "Yeniden doğum sona erdi! ",
  "thig5.8:6.1": "… Kıdemli dervişe Soṇā …. "
}