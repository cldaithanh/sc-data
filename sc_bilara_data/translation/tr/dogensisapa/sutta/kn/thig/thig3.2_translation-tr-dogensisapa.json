{
  "thig3.2:0.1": "Kıdemli Dervişelerin Şarkıları 3.2 ",
  "thig3.2:0.2": "Üçler Silsilesi ",
  "thig3.2:0.3": "Uttamā'nın Şarkısı ",
  "thig3.2:1.1": "Bi' dört-beş sefer oldu, ",
  "thig3.2:1.2": "Terkettim ikametimi. ",
  "thig3.2:1.3": "Gönlümde ne sükûnet ",
  "thig3.2:1.4": "Ne de bir nizam vardı. ",
  "thig3.2:2.1": "Sonra gittim bir dervişeye, ",
  "thig3.2:2.2": "Güvenirdim kendisine; ",
  "thig3.2:2.3": "Bana öğretti hakikati, ",
  "thig3.2:2.4": "Küme, alan, elementleri. ",
  "thig3.2:3.1": "Hakikati dinleyip de ",
  "thig3.2:3.2": "Sonra yedi gün boyunca ",
  "thig3.2:3.3": "Kıpramadan oturdum; ",
  "thig3.2:3.4": "Neşe ve keyif doldum. ",
  "thig3.2:3.5": "Sekizinci gün ayak uzattım- ",
  "thig3.2:3.6": "Kara kümeleri parçaladım. ",
  "thig3.2:4.1": "… Kıdemli dervişe Uttamā …. "
}