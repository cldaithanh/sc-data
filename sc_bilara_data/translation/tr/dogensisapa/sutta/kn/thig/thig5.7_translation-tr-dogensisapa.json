{
  "thig5.7:0.1": "Kıdemli Dervişelerin Şarkıları 5.7 ",
  "thig5.7:0.2": "Beşler Silsilesi ",
  "thig5.7:0.3": "Sakulā'nın Şarkısı ",
  "thig5.7:1.1": "Evli barklı yaşar giderken ",
  "thig5.7:1.2": "Bir dervişeden duydum hakikati; ",
  "thig5.7:1.3": "Bozulmaz adımı gördüm: ",
  "thig5.7:1.4": "Sönümü, tertemiz hakikati. ",
  "thig5.7:2.1": "Oğlumu, kızımı, paramı, tahılımı ",
  "thig5.7:2.2": "Böylece geride bırakıp ",
  "thig5.7:2.3": "Bu evsiz yola düştüm ",
  "thig5.7:2.4": "Saçlarımı da kazıyıp. ",
  "thig5.7:3.1": "Öğrenciliğim boyunca ",
  "thig5.7:3.2": "Dosdoğru yolu yeşerttim; ",
  "thig5.7:3.3": "Tutku ve öfke veren ",
  "thig5.7:3.4": "Lekeleri de terkettim. ",
  "thig5.7:4.1": "Dervişelik makamına varınca ",
  "thig5.7:4.2": "Hatırladım geçmiş yaşamlarımı: ",
  "thig5.7:4.3": "İlâhî görüm temizlendi ",
  "thig5.7:4.4": "Tastamam ve tertemizce. ",
  "thig5.7:5.1": "Şartlara bağlı gelip giden ",
  "thig5.7:5.2": "Fiiliyata yabancılaşınca ",
  "thig5.7:5.3": "Yokettim lekelerimi, ",
  "thig5.7:5.4": "Soğuyup söndüm. ",
  "thig5.7:6.1": "… Kıdemli dervişe Sakulā …. "
}